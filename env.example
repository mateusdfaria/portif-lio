API_TITLE=HospiCast API
API_VERSION=0.2.0
API_ALLOWED_ORIGINS=http://localhost:3000
LOG_LEVEL=INFO
PROMETHEUS_ENABLED=true

# Database Configuration
# Para SQLite (desenvolvimento), deixe vazio ou não defina
# DATABASE_TYPE=sqlite

# Para PostgreSQL (produção), defina a URL completa
# DATABASE_URL=postgresql://user:password@host:port/database
# DATABASE_TYPE=postgresql

# Exemplo para Google Cloud SQL:
# DATABASE_URL=postgresql://hospicast_user:password@/hospicast?host=/cloudsql/PROJECT_ID:REGION:INSTANCE




